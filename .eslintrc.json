{
	"env": {
		"es6": true,
		"node": true,
		"jest/globals": true
	},
	"ignorePatterns": ["build/*"],
	"parserOptions": {
		"ecmaVersion": 2018,
		"sourceType": "module"
	},
	"plugins": ["prettier"],
	"extends": ["prettier", "eslint:recommended", "plugin:import/errors"],
	"rules": {
		"prettier/prettier": "error",
		"import/named": "error"
	}
}